<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="clairecw.example.admin.superclassy">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dashboard"
            android:label="@string/title_activity_dashboard" />
        <activity
            android:name=".Registration"
            android:label="@string/title_activity_registration" />
        <activity
            android:name=".UploadFile"
            android:label="@string/title_activity_upload_file" />
        <activity
            android:name=".Search"
            android:label="@string/title_activity_search" />
        <activity
            android:name=".AccountEdit"
            android:label="@string/title_activity_account_edit" />
        <activity
            android:name=".EditTags"
            android:label="@string/title_activity_edit_tags" />
        <activity android:name=".EditWork" />
        <activity android:name=".ViewUser" />
        <activity android:name=".ViewWork" />
        <activity android:name=".Contact" />
        <activity android:name=".UploadProfilePic" />
        <activity android:name=".Groups" />
        <activity android:name=".CreateGroup" />
        <activity android:name=".ChooseCategory" />
        <activity android:name=".Explore" />
        <activity android:name=".ViewGroup" />
        <activity android:name=".NewPost"></activity>
    </application>

</manifest>
